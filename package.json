{
    "name": "june-bot",
    "version": "0.0.1",
    "author": "Paul Hansa",
    "description": "A cute bot.",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "lint": "eslint ."
    },
    "dependencies": {
        "dialogflow": "latest",
        "discord.js": "latest",
        "express": "latest"
    },
    "devDependencies": {
        "dotenv": "latest",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.1"
    },
    "repository": {
        "url": "https://api.glitch.com/june-bot/git"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "es6": true,
            "node": true
        },
        "extends": [
            "airbnb-base"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "off",
            "no-undef": "off",
            "global-require": "off",
            "no-prototype-builtins": "off",
            "import/no-dynamic-require": "off"
        }
    }
}